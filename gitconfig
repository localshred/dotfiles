[core]
	editor = vim
  excludesfile = ~/.gitignore
[color]
	status = auto
	interactive = auto
	grep = auto
	branch = auto
	diff = auto
[user]
	name = BJ Neilsen
	email = bj.neilsen@gmail.com
[alias]
	st = status
	di = diff
	co = checkout
	ci = commit
	cia = commit -a
	br = branch
	sta = stash
  stl = stash list
  stp = stash pop
	ch = cherry
  cpk = cherry-pick
	yoink = cherry-pick
	ls = ls-remote .
	pu = push
	pgm = push github master
	pum = push upstream master
	phm = push heroku master
	unstage = reset HEAD --
	softrm = rm --cached --
	softrmdir = rm --cached -r --
  logp = log --oneline --abbrev-commit --graph --decorate
	recent = log --oneline -10
	last = log -1 HEAD
	sha = log -1 --oneline HEAD
	mine = !git log --author=`git config user.email`
	difforigin = diff origin/master --name-only
	ctags = !.git/hooks/ctags
  ribbon = tag --force _ribbon origin/master
  catchup = log --patch --reverse --topo-order _ribbon..origin/master
[branch]
	autosetuprebase = remote
[github]
	user = localshred
	token = "some token"
[push]
	default = current
[init]
	templatedir = ~/.git_template
[hub]
	host = git.moneydesktop.com
	protocol = https
[filter "media"]
	clean = git-media-clean %f
	smudge = git-media-smudge %f
