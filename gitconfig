[alias]
	st = status
	di = diff
	co = checkout
	ci = commit
	cia = commit -a
	br = branch
	sta = stash
	ch = cherry
	ls = ls-remote .
	pu = push
	pom = push origin master
	poq = push origin qa
	pos = push origin stage
	pop = push origin stable
	pgm = push github master
	pgq = push github qa
	pgs = push github stage
	pgp = push github stable
	phm = push heroku master
	unstage = reset HEAD --
	softrm = rm --cached --
	softrmdir = rm --cached -r --
	logp = log --oneline
	recent = log --oneline -10
	last = log -1 HEAD
	sha = log -1 --oneline HEAD
	mine = !git log --author=`git config user.email`
	difforigin = diff origin/master --name-only
	ctags = !.git/hooks/ctags
  ribbon = tag --force _ribbon origin/master
  catchup = log --patch --reverse --topo-order _ribbon..origin/master
