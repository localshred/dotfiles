[core]
	editor = vim
  excludesfile = ~/.gitignore
[color]
	status = auto
	interactive = auto
	grep = auto
	branch = auto
	diff = auto
[user]
	name = BJ Neilsen
	email = bj.neilsen@gmail.com
[alias]
  amend      = commit --amend
  br         = branch
  bra        = branch -a
  ch         = cherry
  ci         = commit
  cia        = commit -a
  co         = checkout
  ctags      = !.git/hooks/ctags
  di         = diff
  difforigin = diff origin/master --name-only
  last       = log -1 HEAD
  ls         = ls-remote .
  mine       = !git log --author=`git config user.email`
  pgm        = push github master
  phm        = push heroku master
  pu         = push
  pum        = push upstream master
  recent     = log --oneline -10
  sha        = log -1 --oneline HEAD
  softrm     = rm --cached --
  softrmdir  = rm --cached -r --
  st         = status
  sta        = stash
  unstage    = reset HEAD --
  yoink      = cherry-pick
  amend      = commit --amend
  ammend     = commit --amend
  catchup    = log --patch --reverse --topo-order _ribbon..origin/master
  cpk        = cherry-pick
  idff       = diff
  logp       = log --oneline --abbrev-commit --graph --decorate
  ribbon     = tag --force _ribbon origin/master
  stl        = stash list
  stp        = stash pop
[branch]
	autosetuprebase = remote
[github]
	user = localshred
	token = "some token"
[push]
	default = current
[init]
	templatedir = ~/.git_template
[hub]
	host = git.moneydesktop.com
	protocol = https
[filter "media"]
	clean = git-media-clean %f
	smudge = git-media-smudge %f
